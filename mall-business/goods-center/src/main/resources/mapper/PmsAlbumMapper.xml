<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mallplus.goods.mapper.PmsAlbumMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mallplus.common.entity.pms.PmsAlbum">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="cover_pic" property="coverPic" />
        <result column="pic_count" property="picCount" />
        <result column="sort" property="sort" />
        <result column="description" property="description" />
