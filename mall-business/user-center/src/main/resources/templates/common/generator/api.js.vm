import request from '@/utils/request'
export function fetchList(params) {
    return request({
        url: '/${module}/${Module}${className}/list',
        method: 'get',
        params: params
    })
}
export function create${className}(data) {
    return request({
        url: '/${module}/${Module}${className}/create',
        method: 'post',
        data: data
    })
}

export function delete${className}(id) {
    return request({
        url: '/${module}/${Module}${className}/delete/' + id,
        method: 'delete',
    })
}

export function get${className}(id) {
    return request({
        url: '/${module}/${Module}${className}/' + id,
        method: 'get',
    })
}

export function update${className}(id, data) {
    return request({
        url: '/${module}/${Module}${className}/update/' + id,
        method: 'post',
        data: data
    })
}

